<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.xie.work.domain.UseinfoEntity" table="useinfo" schema="medical">
        <id name="id">
            <column name="id" sql-type="bigint(11)"/>
        </id>
        <property name="company">
            <column name="company" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="position">
            <column name="position" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>