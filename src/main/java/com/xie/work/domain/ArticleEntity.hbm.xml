<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.xie.work.domain.ArticleEntity" table="article" schema="medical">
        <id name="id">
            <column name="id" sql-type="bigint(11)"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="content">
            <column name="content" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="attachments">
            <column name="attachments" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="authorId">
            <column name="author_id" sql-type="bigint(11)" not-null="true"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="modifyTime">
            <column name="modify_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="hits">
            <column name="hits" sql-type="bigint(11)" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="decimal(11,2)" precision="11" scale="2" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>