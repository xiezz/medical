<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.xie.work.domain.UserEntity" table="user" schema="medical">
        <id name="id">
            <column name="id" sql-type="bigint(11)"/>
        </id>
        <property name="realname">
            <column name="realname" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="username">
            <column name="username" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="history">
            <column name="history" sql-type="varchar(25)" length="25" not-null="true"/>
        </property>
        <property name="infoId">
            <column name="info_id" sql-type="bigint(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>